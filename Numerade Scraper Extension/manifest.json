{
  "manifest_version": 2,
  "name": "Numerade Scraper",
  "description": "Extension to scrape answers from Numerade.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Copy Source",
    "default_popup": ""
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "manifest_version": 2
}
